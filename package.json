{
  "name": "recog",
  "version": "1.0.0",
  "description": "Command Line Interface for generating React Components",
  "main": "./dist/src/index.js",
  "preferGlobal": true,
  "bin": {
    "recog": "./dist/src/index.js"
  },
  "publishConfig": {
    "access": "public"
  },
  "scripts": {
    "start": "npm run build && node dist/index.js",
    "build": "npx tsc",
    "test": "jest",
    "test-with-coverage": "jest --coverage"
  },
  "keywords": [
    "React",
    "CLI"
  ],
  "author": "Fergus Farrell",
  "license": "ISC",
  "dependencies": {
    "chalk": "^2.4.2",
    "commander": "^2.20.0",
    "typescript": "^3.5.1"
  },
  "devDependencies": {
    "@types/commander": "^2.12.2",
    "@types/jest": "^24.0.13",
    "@types/node": "^12.0.4",
    "jest": "^24.8.0",
    "ts-jest": "^24.0.2",
    "tslint": "^5.17.0",
    "tslint-config-airbnb": "^5.11.1"
  }
}
